      section#elemis.elemis
        include templates/elemis

      section.testimonials
        include templates/testimonials

    footer.footer#footer
      .footer-backdrop

      .grid

        .row
          .cell-xs-12.cell-sm-5.footer-content
            
            address.footer-address
              h1.footer-address-title!= suntrap
              h3.footer-address-phone 01782 799 733

              p.footer-address-content.
                35 Dimsdale Parade East, Wolstanton <br />
                Newcastle-under-Lyme <br />
                ST5 8BX

            form.footer-address-form(action='http://maps.google.com/maps', class='geolocation', method='get', target='_blank')
