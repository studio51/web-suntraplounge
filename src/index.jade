      section#about.about
        include templates/about

      section#services.services.services--vertical

        .services--categories-wrapper
          .services--categories-inner
            
            .row
              .cell-xs-12.cell-md-3.service-category.tanning(data-target='tanning', style='background-image: url(img/categories/tanning.jpg)')
                img(src='img/categories/tanning.jpg')
                h1.service-category--title Tanning
              
              .cell-xs-12.cell-md-6
                .row
                  .cell-xs-12.cell-md-6.service-category.waxing(data-target='waxing', style='background-image: url(img/categories/waxing.png)')
                    img(src='img/categories/waxing.png')
                    h1.service-category--title Lycon Waxing
                  
                  .cell-xs-12.cell-md-6.service-category.vibraxis(data-target='vibraxis', style='background-image: url(img/categories/vibraxis.jpg)')
                    img(src='img/categories/vibraxis.jpg')
                    h1.service-category--title Vibraxis
                  
                .row
                  .cell-xs-12.service-category.beauty(data-target='beauty', style='background-image: url(img/categories/beauty.jpg)')
                    img(src='img/categories/beauty.jpg')
                    h1.service-category--title Beauty
                
              .cell-xs-12.cell-md-3.service-category.massage(data-target='massage', style='background-image: url(img/categories/massage.jpg)')
                img(src='img/categories/massage.jpg')
                h1.service-category--title Massage
                
            .row
              .cell-xs-12.cell-md-3.service-category.facial(data-target='facial', style='background-image: url(img/categories/facial.png)')
                img(src='img/categories/facial.png')
                h1.service-category--title Facial
                
              .cell-xs-12.cell-md-6.service-category.nails(data-target='nails', style='background-image: url(img/categories/nails.jpg)')
                img(src='img/categories/nails.jpg')
                h1.service-category--title Manicure, Pedicure &amp; Nail Technology
                
              .cell-xs-12.cell-md-3.service-category.aestethic(data-target='aestethic', style='background-image: url(img/categories/aestethic.jpg)')
                img(src='img/categories/aestethic.jpg')
                h1.service-category--title Aesthetics
                    
        .services--content
          
          - var tanning   = { name: 'tanning', format: 'jpg', title: 'Massage Therapy' }
          - var waxing    = { name: 'waxing', format: 'png', title: 'Lycon Waxing' }
          - var vibraxis  = { name: 'vibraxis', format: 'jpg', title: 'Vibraxis' }
          - var beauty    = { name: 'beauty', format: 'jpg', title: 'Beauty' }
          - var massage   = { name: 'massage', format: 'jpg', title: 'Massage' }
          - var facial    = { name: 'facial', format: 'png', title: 'SkinBase Facial' }
          - var nails     = { name: 'nails', format: 'jpg', title: 'Manicure, Pedicure &amp; Nail Technology' }
          - var aestethic = { name: 'aestethic', format: 'jpg', title: 'Aestethic' }
          
          each service in [tanning, waxing, vibraxis, beauty, massage, facial, nails, aestethic]
            
            .service-content--page(style="background-image: url(img/categories/#{service.name}.#{service.format})", id="#{service.name}", class="#{service.name}")
              .grid
                
                .service-content--page--inner
                  .service-content--page--header
                    h1.service-content--page--title #{service.title}
                    
                    span.close-page

                      svg(width='60px', height='60px', viewbox='0 0 60 60', xmlns='http://www.w3.org/2000/svg')
                        g(stroke='none', stroke-width='1', fill='none', fill-rule='evenodd', sketch:type='MSPage')
                          g.circle(sketch:type='MSLayerGroup', transform='translate(-13.000000, -13.000000)', fill='#D8D8D8')
